<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="127.0" prefWidth="501.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="serverclient.ClientController">
   <children>
      <Label layoutX="22.0" layoutY="26.0" text="Choose your opponent !" />
      <TextField fx:id="opponent" layoutX="207.0" layoutY="21.0" prefWidth="234.0" />
      <Button layoutX="371.0" layoutY="80.0" mnemonicParsing="false" onAction="#checkOpponent" text="Confirm" />
      <Button layoutX="207.0" layoutY="80.0" mnemonicParsing="false" onAction="#cancel" text="Cancel" />
      <ChoiceBox fx:id="choiceGames" layoutX="22.0" layoutY="80.0" prefWidth="150.0" />
   </children>
</AnchorPane>
